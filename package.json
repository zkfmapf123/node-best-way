{
  "name": "express-inversify-boilerplate",
  "version": "1.0.0",
  "description": "## Description",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir dist --extensions \".ts\" --source-maps inline",
    "build-watch": "babel -w ./src --out-dir lib --extensions \".ts\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev-start": "npm run build && cross-env NODE_ENV=dev node ./dist/index.js",
    "prod-start": "npm run build && cross-env NODE_ENV=prod node ./dist/index.js",
    "start-simple": "ts-node ./simple_server.ts",
    "normal-server": "npm run build && cross-env NODE_ENV=dev node ./dist/index.js",
    "pm2-dev-start": "pm2-runtime kill && npm run build && cross-env NODE_ENV=dev pm2 start ./dist/index.js -i 6",
    "pm2-prod-start": "pm2 kill && npm run build && cross-env NODE_ENV=prod pm2 start ./dist/index.js -i 6"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/amqplib": "^0.8.2",
    "@types/express": "^4.17.14",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.24.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.19.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@types/node": "^18.7.19",
    "amqplib": "^0.8.0",
    "await-to-js": "^3.0.0",
    "babel-plugin-inline-json-import": "^0.3.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-parameter-decorator": "^1.0.16",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "fp-ts": "^2.12.3",
    "helmet": "^6.0.0",
    "inversify": "^6.0.1",
    "pm2": "^5.2.0",
    "reflect-metadata": "^0.1.13",
    "serializr": "^2.0.5",
    "wait": "^0.4.2"
  },
  "directories": {
    "lib": "lib"
  }
}
